			If @error OR NOT $aret[0] Then Return SetError(@error + 30, @extended, 0)
			Switch $iindex
				Case 1, 4
					Return $tdata
				Case Else
					Return DllStructGetData($tdata, 1)
			EndSwitch
		EndFunc

