													ConsoleWrite("+*3: " & $trnpassed & " " & $trn2[3] & @LF)
													IniWrite($inifile, "registered", $trnpassed & " " & $trn2[3], $trn2[1] & "~" & $trn2[2] & "~" & $trn2[4])
