								$trnlobbyhnd1 = WinGetHandle("[REGEXPTITLE:\ATournament.*-.*\z]")
								WinClose($trnlobbyhnd1)
							EndIf
						EndIf
						closelobbies()
						If $lobbyminimize == 1 Then WinSetState($hlobby, "", @SW_MINIMIZE)
						$totalbuyin = Number($totalbuyin) + Number($thisbuyin)
						Return True
					Else
						closelobbies()
						$tourneyregok = False
						Return False
					EndIf
					logit("EN808")
					Sleep(1000)
					logit("IF809")
					If $tourneyregok = True Then
						Return True
					Else
						$tourneyregok = False
						Return False
					EndIf
