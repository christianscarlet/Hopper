								fconsolewrite("Done avoiding non joined table " & $safename & " (missing mins)")
							EndIf
							logit("EN731")
						EndIf
						logit("EN732")
						$avoidfulltables[$a] = ""
						If $avoidfulltables[$a] == "" AND UBound($avoidfulltables) > 1 Then
							ContinueLoop
						EndIf
					EndIf
					logit("EN733")
				Else
					ContinueLoop
				EndIf
