Func buttonstateset($varsetting, $varname, $state = 4)
	If IniRead($inifile, "Settings", $varsetting, $state) == 1 Then
		GUICtrlSetState($varname, $gui_checked)
	Else
		GUICtrlSetState($varname, $gui_unchecked)
	EndIf
EndFunc

