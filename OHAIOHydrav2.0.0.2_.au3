		Case Else
			$__g_itifcolordepth = 0
	EndSwitch
EndFunc

Func _screencapture_settifcompression($icompress)
	Switch $icompress
		Case 1
			$__g_itifcompression = $gdip_evtcompressionnone
		Case 2
			$__g_itifcompression = $gdip_evtcompressionlzw
		Case Else
			$__g_itifcompression = 0
	EndSwitch
EndFunc

