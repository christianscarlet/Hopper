Func _getdpi()
	Local $a1[3]
	Local $hdc, $aret
	Local $idpi, $idpirat, $logpixelsy = 90, $hwnd = 0
	$hdc = DllCall("user32.dll", "long", "GetDC", "long", $hwnd)
	$aret = DllCall("gdi32.dll", "long", "GetDeviceCaps", "long", $hdc[0], "long", $logpixelsy)
	$hdc = DllCall("user32.dll", "long", "ReleaseDC", "long", $hwnd, "long", $hdc)
	$idpi = $aret[0]
	Select
		Case $idpi = 0
			$idpi = 96
			$idpirat = 94
		Case $idpi < 84
			$idpirat = $idpi / 105
		Case $idpi < 121
			$idpirat = $idpi / 96
		Case $idpi < 145
			$idpirat = $idpi / 95
		Case Else
			$idpirat = $idpi / 94
	EndSelect
	$a1[0] = 2
	$a1[1] = $idpi
	$a1[2] = $idpirat
	Return $a1
EndFunc

