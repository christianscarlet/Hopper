	If NOT IsArray($icolor) OR $icolor[0] == -1 Then Return SetError(1, 0, -1)
	Local $scolor = Hex($icolor[0], 6)
	Return StringRight($scolor, 2) & StringMid($scolor, 3, 2) & StringLeft($scolor, 2)
