			If IsArray(PixelSearch(656 + $offset[0], 498 + $offset[1], 656 + $offset[0], 498 + $offset[1], 1710618, 5, 1, $htable[$a])) AND IsArray(PixelSearch(454 + $offset[0], 557 + $offset[1], 454 + $offset[0], 557 + $offset[1], 16777215, 5, 1, $htable[$a])) Then
