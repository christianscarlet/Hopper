		$ibuffer = _sendmessage($hwnd, $tb_getbuttontextw, $icommandid)
	Else
		$ibuffer = _sendmessage($hwnd, $tb_getbuttontexta, $icommandid)
	EndIf
	If $ibuffer = 0 Then Return SetError(True, 0, "")
	If $ibuffer = 1 Then Return SetError(False, 0, "")
