						Do
							Sleep(100)
							$dbicount += 1
							$buyinsucc = detectbuyin()
						Until $buyinsucc == True OR $dbicount > 15
						If $buyinsucc == False Then
							$tourneyregok = False
							closelobbies()
							Return
						EndIf
					ElseIf detectregclosed($hlobby) Then
						fconsolewrite("Registration closed window found")
