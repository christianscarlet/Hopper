		Local $timestamp
		Local $commaat
		$commaat = 0
		logit("FO723")
		Local $safename
		If UBound($avoidtables) > 1 Then
			For $a = 0 To UBound($avoidfulltables) - 1
				If $avoidfulltables[$a] == "" Then
					ContinueLoop
				EndIf
				$timestamp = $avoidfulltables[$a]
				logit("IF724")
				If $avoidfulltables[$a] == "" AND UBound($avoidfulltables) > 1 Then
					ContinueLoop
				EndIf
				logit("EN725")
