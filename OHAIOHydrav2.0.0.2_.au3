		$smenu = StringReplace(_guictrlmenu_getitemtext($hmenu, $ii), "&", "")
		Switch $binstr
			Case False
				If $smenu = $stext Then Return $ii
			Case True
				If StringInStr($smenu, $stext) Then Return $ii
		EndSwitch
	Next
	Return -1
EndFunc

