									If $checkdeadtableattempts[$a] >= IniRead($inifile, "settings", "DeadTableAttempts", 2) Then
										fconsolewrite($titlepoint & " is inactive")
										fconsolewrite("Closing inactive table: " & WinGetTitle($htable[$a]))
