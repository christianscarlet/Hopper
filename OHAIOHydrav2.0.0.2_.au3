								MouseClick("left", $winpos[0] + 101, $winpos[1] + 73, 1, 0)
								Return
							EndIf
						EndIf
					Next
				EndIf
			EndIf
		Else
			$hlob = getlobbyhnd()
			fconsolewrite("Close IPoker (WClose)")
			WinActivate($hlob)
			SendKeepActive($hlob)
			Send("!+{f4}")
			Sleep(1000)
