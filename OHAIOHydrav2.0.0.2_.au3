											ConsoleWrite("+*2: " & $trnpassed & " " & $trn2[3] & @LF)
											IniWrite($inifile, "registered", $trnpassed & " " & $trn2[3], $trn2[1] & "~" & StringReplace(StringReplace($trn2[2], "00/", @MON & "/"), "/00", "/" & @MDAY) & "~" & $trn2[4])
										Else
