			Local $aret = DllCall("user32.dll", "bool", "GetUserObjectInformationW", "handle", $hobject, "int", $iindex, "ptr", 0, "dword", 0, "dword*", 0)
			If @error OR NOT $aret[5] Then Return SetError(@error + 10, @extended, 0)
